<div class="containerCfor">
        <h1for>Cadastro de Fornecedor</h1for>
        <form action="/Fornecedor/Cadastro" method="post">
            

            <div class="form-groupfor">
                <labelfor for="nome">Nome:</labelfor>
                <input type="text" id="nome" name="nome" required>
            </div>

            <div class="form-groupfor">
                <label for="telefone">Telefone:</label>
                <input type="tel" id="telefone" name="telefone" required>
            </div>

            <div class="form-groupfor">
                <labelfor for="estado">Estado:</labelfor>
                <input type="text" id="estado" name="estado" required>
            </div>

            <div class="form-groupfor">
                <labelfor for="cidade">Cidade:</labelfor>
                <input type="text" id="cidade" name="cidade" required>
            </div>

            <div class="form-groupfor">
                <labelfor for="cnpj">CNPJ:</labelfor>
                <input type="text" id="cnpj" name="cnpj" required>
            </div>

            <div class="buttonsfor">
                <button type="submit" class="btnfor salvarfor">Salvar</button>
                <button type="button" class="btnfor voltarfor" onclick="window.location.href='/Fornecedor';">Voltar</button>
            </div>
        </form>
    </div>
